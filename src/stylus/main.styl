/** Stylus Styles */
@import '../../node_modules/vuetify/src/stylus/main';

html, body {
  overflow-y: auto!important;
  padding: 0!important;
  margin: 0!important;
}

.ui-logo {
  height: 56px;
}

.ui-process-percent {
  display: block;
  margin-top: 12px;
}

thead th {
  color: $grey.darken-3!important;
}

td, th {
  white-space: nowrap;
}

.router-link-exact-active {
  background: rgba(0,0,0,0.22);
}
